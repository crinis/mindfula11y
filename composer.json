{
    "name": "mindfulmarkup/mindfula11y",
    "description": "TYPO3 extension with backend tools to help you create accessible content. Includes management of heading structure, finding missing alternative text for images and alternative text generation using ChatGPT. More features to come.",
    "type": "typo3-cms-extension",
    "license": [
        "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Felix Spittel",
            "role": "Developer",
            "homepage": "https://www.mindfulmarkup.de"
        }
    ],
    "support": {
        "issues": "https://github.com/crinis/mindfula11y/issues",
        "source": "https://github.com/crinis/mindfula11y"
    },
    "require": {
        "php": ">=8.2 <=8.4",
        "typo3/cms-core": "~13.4.0",
        "typo3/cms-backend": "~13.4.0",
        "typo3/cms-fluid": "~13.4.0",
        "typo3/cms-frontend": "~13.4.0",
        "typo3/cms-extbase": "~13.4.0"
    },
    "autoload": {
        "psr-4": {
            "MindfulMarkup\\MindfulA11y\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "mindfula11y"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": false,
            "typo3/class-alias-loader": false
        }
    },
    "keywords": [
        "TYPO3",
        "accessibility",
        "alt text",
        "extension",
        "chatgpt"
    ],
    "minimum-stability": "alpha"
}
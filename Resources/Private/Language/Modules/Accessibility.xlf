            <trans-unit id="menu.tables.all" resname="menu.tables.all">
                <source>All Record Types</source>
            </trans-unit>

            <trans-unit id="menu.filter" resname="menu.filter">
                <source>Filter</source>
            </trans-unit>
            <trans-unit id="menu.filter.fileMetaData" resname="menu.filter.fileMetaData">
                <source>
                    Filter records with inherited alternative text
                </source>
            </trans-unit>

            <trans-unit id="menu.features" resname="menu.features">
                <source>Feature</source>

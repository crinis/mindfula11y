            </trans-unit>
            <trans-unit id="mlang_labels_tabdescr" resname="mlang_labels_tabdescr">
                <source>
                    Checks for various accessibility issues on the currently selected page.
                </source>
            </trans-unit>
            <trans-unit id="mlang_tabs_tab" resname="mlang_tabs_tab">
                <source>
                    Accessibility
                </source>
            </trans-unit>

            <trans-unit id="noPageSelected" resname="noPageSelected">
                <source>
                    No page selected.
                </source>
            </trans-unit>
            <trans-unit id="noPageSelected.description" resname="noPageSelected.description">
                <source>
                    Please select a page in the page tree to check for accessibility issues.
                </source>
            </trans-unit>
            <trans-unit id="noLanguageAccess" resname="noLanguageAccess">
                <source>
                    No access to the selected language.
                </source>
            </trans-unit>
            <trans-unit id="noLanguageAccess.description"
                resname="noLanguageAccess.description">
                <source>
                    Please select an allowed language in the page tree to check for accessibility
                    issues.
                </source>
            </trans-unit>
            <trans-unit id="noTableAccess" resname="noTableAccess">
                <source>No access to the selected table.</source>
            </trans-unit>
            <trans-unit id="noTableAccess.description" resname="noTableAccess.description">
                <source>
                    Please select a record type that you have access to or contact your
                    administrator.
                </source>
            </trans-unit>


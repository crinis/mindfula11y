            <trans-unit id="headingStructure.relation.descendant" resname="headingStructure.relation.descendant">
                <source>Related to parent heading</source>
            </trans-unit>
            <trans-unit id="headingStructure.relation.sibling" resname="headingStructure.relation.sibling">
                <source>Related to sibling heading</source>
            </trans-unit>
            <trans-unit id="headingStructure.relation.notFound" resname="headingStructure.relation.notFound">
                <source>Ancestor not found</source>
            </trans-unit>
            <trans-unit id="headingStructure.relation.notFound.description" resname="headingStructure.relation.notFound.description">
                <source>The referenced ancestor heading could not be found in the document.</source>
            </trans-unit>
            <trans-unit id="headingStructure.noHeadings" resname="headingStructure.noHeadings">

            <trans-unit id="missingAltText" resname="missingAltText">
                <source>Images without alternative text</source>
            </trans-unit>
            <trans-unit id="missingAltText.description" resname="missingAltText.description">
                <source>
                    Listing records of tables that are missing alternative text in their
                    associated file references. Allows filtering by the type and language of the
                    record and can function recursively for all subpages of the currently selected page.
                </source>
            </trans-unit>
            <trans-unit id="missingAltText.generate.button" resname="missingAltText.generate.button">
                <source>Generate</source>
            </trans-unit>
            <trans-unit id="missingAltText.generate.loading"
                resname="missingAltText.generate.loading">
                <source>Generating alternative text...</source>
            </trans-unit>
            <trans-unit id="missingAltText.generate.success"
                resname="missingAltText.generate.success">
                <source>Alternative text generated.</source>
            </trans-unit>
            <trans-unit id="missingAltText.generate.success.description"
                resname="missingAltText.generate.success.description">
                <source>
                    Alternative text was successfully generated. Please make sure it describes
                    the picture accurately.
                </source>
            </trans-unit>
            <trans-unit id="missingAltText.generate.error.unknown"
                resname="missingAltText.generate.error.unknown">
                <source>Unknown error occurred.</source>
            </trans-unit>
            <trans-unit id="missingAltText.generate.error.unknown.description"
                resname="missingAltText.generate.error.unknown.description">
